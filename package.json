{
  "private": true,
  "scripts": {
    "dev": "next",
    "build": " prisma generate && next build",
    "start": "next start",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "eslint": "eslint --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "lint": "npm run eslint -- --fix",
    "migrate": "prisma db push",
    "schema": "prisma generate",
    "update": "pnpx npm-check-updates -u && pnpm i"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "react/no-unescaped-entities": "off"
    }
  },
  "prettier": {
    "semi": false,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "singleQuote": true,
    "useTabs": true
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mantine/carousel": "^6.0.16",
    "@mantine/core": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@notionhq/client": "^2.2.6",
    "@prisma/client": "^4.16.2",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.9.3",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "^7.1.0",
    "framer-motion": "^10.12.18",
    "google-photos-album-image-url-fetch": "^3.1.0",
    "iron-session": "^6.3.1",
    "next": "13.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.10.1",
    "react-tsparticles": "^2.10.1",
    "swr": "^2.2.0",
    "tsparticles": "^2.10.1",
    "tsparticles-engine": "^2.10.1",
    "twilio": "^4.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "prisma": "^4.16.2",
    "typescript": "^5.1.6"
  }
}
