{
	"private": true,
	"scripts": {
		"dev": "next",
		"build": " prisma generate && next build",
		"start": "next start",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
		"eslint": "eslint --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx)\"",
		"format": "npm run prettier -- --write",
		"lint": "npm run eslint -- --fix",
		"migrate": "prisma db push",
		"schema": "prisma generate"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"next/core-web-vitals",
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"rules": {
			"react/no-unescaped-entities": "off"
		}
	},
	"prettier": {
		"semi": false,
		"jsxSingleQuote": true,
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.17",
		"@chakra-ui/react": "2.5.2",
		"@emotion/react": "^11",
		"@emotion/styled": "^11",
		"@fontsource/limelight": "^4.5.11",
		"@fontsource/raleway": "^4.5.12",
		"@mantine/carousel": "^6.0.4",
		"@mantine/core": "^6.0.4",
		"@mantine/hooks": "^6.0.4",
		"@prisma/client": "^4.11.0",
		"@tanstack/match-sorter-utils": "^8.7.6",
		"@tanstack/react-table": "^8.7.9",
		"crypto-js": "^4.1.1",
		"date-fns": "^2.29.3",
		"embla-carousel-react": "^7.1.0",
		"framer-motion": "^10",
		"iron-session": "^6.3.1",
		"next": "13.2.4",
		"react": "^18.2.0",
		"react-csv": "^2.2.2",
		"react-dom": "^18.2.0",
		"react-icons": "^4.8.0",
		"react-tsparticles": "^2.9.3",
		"swr": "^2.1.1",
		"tsparticles": "^2.9.3",
		"tsparticles-engine": "^2.9.3",
		"twilio": "^4.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@chakra-ui/styled-system": "^2.6.2",
		"@chakra-ui/system": "^2.5.2",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^18.15.5",
		"@types/react": "^18.0.28",
		"@types/react-csv": "^1.1.3",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-next": "^13.2.4",
		"prettier": "^2.8.6",
		"prisma": "^4.11.0",
		"typescript": "5.0.2"
	}
}
