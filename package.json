{
	"private": true,
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
		"eslint": "eslint --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx)\"",
		"format": "npm run prettier -- --write",
		"lint": "npm run eslint -- --fix",
		"migrate": "prisma db push",
		"schema": "prisma generate"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"next/core-web-vitals",
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended"
		]
	},
	"prettier": {
		"semi": false,
		"jsxSingleQuote": true,
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.15",
		"@chakra-ui/react": "2.4.6",
		"@emotion/react": "^11",
		"@emotion/styled": "^11",
		"@prisma/client": "^4.8.1",
		"@tanstack/match-sorter-utils": "^8.7.2",
		"@tanstack/react-table": "^8.7.4",
		"crypto-js": "^4.1.1",
		"date-fns": "^2.29.3",
		"framer-motion": "^8",
		"iron-session": "^6.3.1",
		"next": "13.1.1",
		"react": "^18.2.0",
		"react-csv": "^2.2.2",
		"react-dom": "^18.2.0",
		"react-icons": "^4.7.1",
		"swr": "^2.0.0",
		"twilio": "^3.84.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@chakra-ui/styled-system": "^2.5.1",
		"@chakra-ui/system": "^2.3.7",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-csv": "^1.1.3",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"eslint": "^8.31.0",
		"eslint-config-next": "^13.1.1",
		"prettier": "^2.8.2",
		"prisma": "^4.8.1",
		"typescript": "4.9.4"
	}
}
